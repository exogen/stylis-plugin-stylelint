{
  "name": "stylis-plugin-stylelint",
  "version": "0.1.0",
  "description": "Stylis plugin/middleware for running stylelint",
  "author": "Brian Beck <exogen@gmail.com>",
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "format": "prettier --write \"**/*.{js,md}\"",
    "precommit": "lint-staged && npm test",
    "test": "jest --verbose"
  },
  "lint-staged": {
    "**/*.{js,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "peerDependencies": {
    "stylelint": "^9.1.1"
  },
  "dependencies": {
    "execa": "^0.9.0",
    "get-stdin": "^6.0.0",
    "strip-indent": "^2.0.0"
  },
  "devDependencies": {
    "babel-plugin-styled-components": "^1.5.1",
    "babel-preset-react": "^6.24.1",
    "husky": "^0.14.3",
    "jest": "^22.4.2",
    "lint-staged": "^7.0.0",
    "prettier": "^1.11.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-test-renderer": "^16.2.0",
    "styled-components": "^3.1.6",
    "stylelint": "^9.1.1",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylis": "^3.4.10"
  }
}
